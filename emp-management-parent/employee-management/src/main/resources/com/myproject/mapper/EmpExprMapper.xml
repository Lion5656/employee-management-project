<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--放置對應EmpExprMapper限定類別名-->
<mapper namespace="com.myproject.mapper.EmpExprMapper">
    <insert id="insertBatch">
        insert into emp_expr(emp_id, company, job, begin, end) values
        <foreach collection="exprList" item="expr" separator=",">
            (#{expr.empId}, #{expr.company}, #{expr.job}, #{expr.begin}, #{expr.end})
        </foreach>
    </insert>

    <delete id="deleteByEmpIds" parameterType="List">
        delete from emp_expr where emp_id in
        <foreach collection="empIdList" item="empId" open="(" close=")" separator=",">
            #{empId}
        </foreach>
    </delete>
</mapper>